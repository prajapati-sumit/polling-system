<%- include('partials/header'); -%>


<div class="row" >
  <div class="col-sm-6">
    <div class="card " >
      <div class="card-body " >
        <p class="card-text">Create your own poll</p>
        <form action="/createpolls/create" method="GET" class="homeforms">
          <button class="btn" id="create">Create</button>
        </form>
      </div>
    </div>
  </div>
  <div class="col-sm-6">
    <div class="card">
      <div class="card-body " >
        <p class="card-text">Cast Your Vote</p>
        <form action="/castvote" method="POST" class="homeforms">
          <input type="text" name="pollId" required class="pollid" />
          <button class="btn" id="gotopoll">Go to poll</button>
        </form>
      </div>
    </div>
  </div>
</div>



<%- include('partials/footer'); -%>